{
	"name": "ScarpLiveBetterSettings",
	"version": "1.0.0",
	"description": "Extension for add more settings on scarp.live",
	"manifest_version": 2,
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"64": "icon64.png",
		"128": "icon128.png"
	},
	"permissions": [],
	"content_scripts": [
		{
			"matches": ["https://scalp.live/app*"],
			"js": ["./build/bundle.js"]
		}
	]
}
